{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4, {
                "SwitchCase": 1
            }
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars": [
            "error", {
                "args": "none"
            }
        ],
        "no-var": "error",
        "prefer-const": "error",
        "no-console": "off"
    },
    "globals": {
        "angular": false,
        "$": false
    }
}